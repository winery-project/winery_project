<h1>ジャンル編集</h1>
<%= render 'admin/shared/error_messages', resource: @genre %>
<%= form_with model: [:admin, @genre] do |f| %>
  <div class="row">
    <div class="from-group col-sm-4">
      <%= f.text_field :name, class:"form-control" %>
    </div>
    <div class="from-group col-sm-4">
      <%= f.submit 'Edit genre', class:"btn btn-warning" %>
    </div>
  </div>
<% end %>
<div>
  <%= link_to 'Delete', admin_genre_path, method: :delete %>
</div>

