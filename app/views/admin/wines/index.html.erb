<h1>ワイン一覧</h1>
<%= link_to 'ワインを新規追加', new_admin_wine_path %>

<div class="container">
  <% @wines.each do |wine| %>
  <div class="row">
    <div class="col-sm-4"><li><%= wine.name %></li></div>
    <div class="col-sm-2"><%= link_to 'Show', admin_wine_path(wine.id) %></div>
    <div class="col-sm-2"><%= link_to 'Edit', edit_admin_wine_path(wine.id) %></div>
  </div>
  <% end %>
</div>


